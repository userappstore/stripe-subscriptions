{
  "name": "@userappstore/stripe-subscriptions",
  "version": "1.2.0",
  "main": "index.js",
  "description": "Stripe subscriptions module for Dashboard",
  "engine": {
    "node": "8.1.14",
    "npm": "5.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/userappstore/stripe-subscriptions.git"
  },
  "license": "SPDX(MIT OR Unlicense)",
  "scripts": {
    "start": "node main.js",
    "test": "NODE_ENV=testing mocha src --recursive --timeout 180000 --slow 180000"
  },
  "dependencies": {
    "@userappstore/dashboard": "^1.2.0",
    "@userappstore/maxmind-geoip": "^1.0.1067",
    "stripe": "^4.25.0"
  },
  "dashboard": {
    "menus": {
      "administrator": [
        {
          "href": "/administrator/subscriptions",
          "text": "Stripe Subscriptions",
          "object": "link"
        }
      ],
      "account": [
        {
          "href": "/account/subscriptions",
          "text": "My subscriptions",
          "object": "link"
        }
      ]
    },
    "modules": [
      "@userappstore/dashboard",
      "@userappstore/maxmind-geoip"
    ],
    "server": [
      "/src/server/bind-stripekey.js",
      "/src/server/bind-customer.js",
      "/src/server/require-subscription.js",
      "/src/server/require-payment.js"
    ]
  }
}
